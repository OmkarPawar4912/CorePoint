@model IEnumerable<CorePoint.Service.ViewModel.ViewModelIncident>

<div class="pagetitle">
    <div class="card">
        <div class="card-body-head">
            <div class="float-left">
                <h5 class="card-title">Incident</h5>
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            </div>
            <div class="float-right">
                <a asp-action="Create" class="btn btn-primary">Create Employee</a>
            </div>
        </div>
    </div>
</div>

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <table class="table table-striped table-responsive datatable">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IncidentType)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Severity)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IsConfidence)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Shift)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CreateBy)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CreateDate)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.UpdateBy)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.UpdateDate)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SupervisorId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IncidentType)
                                    </td>
                                  
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Severity)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IsConfidence)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IncidentDate)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CreateDate)
                                    </td>
                                    <td>
                                        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                                        <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                                        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>

