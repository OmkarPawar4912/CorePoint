@model IEnumerable<CorePoint.Service.ViewModel.ViewModelIncidentStatus>
<div class="pagetitle">
    <div class="card">
        <div class="card-body-head">
            <div class="float-left">
                <h5 class="card-title">Incident List</h5>
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            </div>
            <div class="float-right">
                <a asp-action="Create" class="btn btn-primary">Create Incident</a>
            </div>
        </div>
    </div>
</div>

<section class="section">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body-table">
                    <table class="table table-striped table-responsive datatable">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.vmId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.vmLateststatus)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.vmIncidentType)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.vmIncidentType)                               
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.vmServertiy)
                                </th>
                               <th>
                                    @Html.DisplayNameFor(model => model.vmIncidentDate)
                                </th>
                                 <th>
                                    @Html.DisplayNameFor(model => model.vmIncidentReportDate)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                  <td>
                                    @Html.DisplayFor(model => @item.vmId)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => @item.vmLateststatus)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => @item.vmIncidentType)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => @item.vmIncidentType)                               
                                </td>
                                <td>
                                    @Html.DisplayFor(model => @item.vmServertiy)
                                </td>
                               <td>
                                    @Html.DisplayFor(model => @item.vmIncidentDate)
                                </td>
                                 <td>
                                    @Html.DisplayFor(model => @item.vmIncidentReportDate)
                                </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
